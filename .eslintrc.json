{
    "env": {
        "node": true,
        "es2020": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
    "eol-last": ["error", "always"],
    "comma-dangle": [
      2,
      "never"
    ],
    "no-param-reassign": 0,
    "import/prefer-default-export": "off",
    "no-underscore-dangle": 0,
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "complexity": [
      2,
      {
        "max": 20
      }
    ],
    "camelcase": 0,
    "class-methods-use-this": 0,
    "curly": 2,
    "eqeqeq": [
      2,
      "allow-null"
    ],
    "global-require": 0,
    "handle-callback-err": 2,
    "indent": [
      2,
      2
    ],
    "key-spacing": 2,
    "max-len": [
      2,
      100,
      2,
      {
        "ignoreRegExpLiterals": true
      }
    ],
    "max-nested-callbacks": [
      2,
      10
    ],
    "max-params": [
      2,
      6
    ],
    "max-statements": [
      2,
      52
    ],
    "new-cap": 2,
    "no-cond-assign": 2,
    "no-extend-native": 2,
    "no-mixed-requires": 2,
    "no-new-require": 2,
    "no-proto": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-shadow-restricted-names": 2,
    "no-throw-literal": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "no-use-before-define": [
      2,
      {
        "functions": true,
        "classes": true
      }
    ],
    "no-useless-escape": 2,
    "no-var": 2,
    "object-curly-spacing": [
      2,
      "always"
    ],
    "quotes": 2,
    "semi-spacing": 2,
    "no-console": "off",
    "space-before-function-paren": 2,
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "strict": 2
  }
}
