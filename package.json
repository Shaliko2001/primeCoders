{
  "name": "site",
  "version": "1.0.0",
  "description": "CITY",
  "scripts": {
    "start": "babel-node src/bin/www.js",
    "dev": "babel-watch src/bin/www.js",

    "migrations": "babel-node migrations/1_create_tables.js",
    "migrations-down": "babel-node migrations/1_drop_tables.js",
    "seed-migrations": "babel-node seeds/seed.js",
    
    "migrate-arm": "babel-node migrations_arm/1_create_tables.js",
    "migrate-down-arm": "babel-node migrations_arm/1_drop_tables.js",
    "seed-arm": "babel-node seeds_arm/seed.js",
    

    "migrate-en": "babel-node migrations_en/1_create_tables.js",
    "migrate-down-en": "babel-node migrations_en/1_drop_tables.js",
    "seed-en": "babel-node seeds_en/seed.js",

    "migrate": "npm run migrations & npm run migrate-arm & npm run migrate-en",
    "migrate-down": "concurrently \"npm run migrations-down\" \"npm run migrate-down-arm\"  \"npm run migrate-down-en\"",
    "lint-no-fix": "eslint src/",
    "lint": "eslint --fix src/"
  },
  "author": "Engined AM",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^7.9.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1"
  },
  "dependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/node": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@meanie/mongoose-to-json": "^2.6.0",
    "babel": "^6.23.0",
    "babel-watch": "^7.7.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "eslint-config-airbnb": "^18.2.0",
    "express": "^4.17.3",
    "express-session": "^1.17.3",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "mongoose": "^6.2.2",
    "mongoose-hidden": "^1.9.1",
    "mongoose-id-validator": "^0.6.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "nodemailer": "^6.9.3",
    "objection": "^3.0.1",
    "parse-duration": "^1.0.2",
    "pg": "^8.8.0",
